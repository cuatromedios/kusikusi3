{
    "name": "cuatromedios/kusikusi",
    "description": "API-first applications based on hierarchical data",
    "keywords": ["framework", "Cuatromedios", "Api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.7.*",
        "vlucas/phpdotenv": "~2.2",
        "ramsey/uuid": "^3.7",
        "hackzilla/password-generator": "^1.4",
        "cuatromedios/kusikusi-php-kernel": "^0.19.3",
        "league/flysystem": "^1.0",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "files": [
            "vendor/cuatromedios/kusikusi-php-kernel/src/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@kusikusiConfig"
        ],
        "kusikusiConfig": "@php artisan kusikusi:post-install"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
